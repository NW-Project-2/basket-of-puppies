<header id="header">
	<div class="container">
		<div class="row">
			<div class="col-md-10">
				<h1><span class="fa fa-cog"></span> Dashboard <small>Manage your
						site</small></h1>

			</div>
			<div class="col-md-2">
				<div class="dropdown create">
					<button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1"
						data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
						Create Content
						<span class="caret"></span>
					</button>
					<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
						<li><a type="button" data-toggle="modal" data-target="#addPage">Add Page</a></li>
						<li><a href="#">Add Dog</a></li>
						<li><a href="#">Add Owner</a></li>
					</ul>
				</div>
			</div>
		</div>
	</div>




</header>

<div class="container">
	<div class="row">
		<div class="col-12">
			<div class="row">
				<button type="button" class="btn btn-warning btn-lg add-btn addNewOwner">Add New Owner<br><img
						src="/assets/img/owner-1.png"></button>
				<button type="button" class="btn btn-info btn-lg add-btn addNewDog">Add New Dog<br><img
						src="/assets/img/puppy3.png">
				</button>
			</div>
			<div class="row">
				<button type="button" class="btn btn-success btn-lg add-btn updateAllDogsActivity">Update Activities for ALL Dogs<img
						src="/assets/img/puppy-potty.png"></button>
				<button type="button" class="btn btn-dark btn-lg add-btn updateDogActivity">Update Activities for ONE Dog<img
						src="/assets/img/puppies.png"></button>
			</div>
			<div class="row">
				<button type="button" class="btn btn-danger btn-lg add-btn viewAllDogs">View All Dogs</button>
				<button type="button" class="btn btn-primary btn-lg add-btn viewAllOwners">View All Owners</button>
			</div>
		</div>
	</div>
</div>

